";\r\nimport { getTop } from \"../../api/font\";\r\nexport default {\r\n  name: \"FNotice\",\r\n  data() {\r\n    return {\r\n      articles: [],\r\n      channels: {},\r\n    };\r\n  },\r\n  created() {\r\n    queryByPos(\"C\")\r\n      .then((data) => {\r\n        let arr = data.data;\r\n        this.channels = arr[0];\r\n        console.log(arr[0]);\r\n        getTop(this.channels.id, 4).then((data) => {\r\n          this.articles = data.list;\r\n        });\r\n      })\r\n      .catch(() => {});\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.notice ul {\r\n  padding-top: 18px;\r\n}\r\n.notice li {\r\n  font-size: 12px;\r\n  line-height: 30px;\r\n  margin-bottom: 12px;\r\n  display: block;\r\n  -moz-border-radius: 4px;\r\n  border-radius: 4px;\r\n  background: #f6f6f6;\r\n  padding: 4px 12px 4px 30px;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n  position: relative;\r\n  -moz-transition: all 0.2s ease;\r\n  -webkit-transition: all 0.2s ease;\r\n  transition: all 0.2s ease;\r\n}\r\n.notice li:before {\r\n  position: absolute;\r\n  content: \"\";\r\n  width: 3px;\r\n  height: 3px;\r\n  background: #000;\r\n  border-radius: 10px;\r\n  left: 15px;\r\n  top: 20px;\r\n}\r\n.notice li:hover {\r\n  background: #f2f2f2;\r\n  box-shadow: 0 0 10px #ccc;\r\n  -moz-transition: all 0.2s ease;\r\n  -webkit-transition: all 0.2s ease;\r\n  transition: all 0.2s ease;\r\n}\r\n</style>"]}]}