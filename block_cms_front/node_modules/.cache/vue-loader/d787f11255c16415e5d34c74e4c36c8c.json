;\r\n      }\r\n      if (command == \"logout\") {\r\n        this.$store.commit(\"logout\");\r\n        this.$router.push(\"/login\");\r\n      }\r\n      if (command == \"password\") {\r\n        this.passVisiable = true;\r\n      }\r\n    },\r\n    hidden() {\r\n      this.passVisiable = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.index_header {\r\n  background: cadetblue;\r\n  width: 100%;\r\n}\r\n.logo {\r\n  height: 60px;\r\n  color: #fff;\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  border-spacing: 0px;\r\n}\r\n.logo img {\r\n  width: 45px;\r\n  height: 45px;\r\n}\r\n.user {\r\n  color: #ffffff;\r\n  font-size: 14px;\r\n}\r\n.user img {\r\n  border-radius: 50%;\r\n}\r\n.el-link.el-link--default {\r\n  color: #fff;\r\n}\r\n</style>"]}]}